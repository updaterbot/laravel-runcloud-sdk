{
  "name": "updaterbot/laravel-runcloud-sdk",
  "description": "Laravel package wrapper around the runcloud API SDK",
  "version": "1.0.2",
  "license": "MIT",
  "authors": [
    {
      "name": "Timothy Anderson",
      "email": "info@creativesunderground.com"
    },{
      "name": "bicsakszerk",
      "email": "bicsakszerk@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "updaterbot/runcloud-sdk": "v1.1.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mindfullsilence\\LaravelRuncloudSdk\\Providers\\RuncloudClientProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Mindfullsilence\\LaravelRuncloudSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mindfullsilence\\LaravelRuncloudSdk\\": "src/",
      "Mindfullsilence\\LaravelRuncloudSdk\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.x-dev",
    "orchestra/testbench": "3.8.x-dev"
  }
}
